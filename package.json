{
  "name": "postcss-var-optimize",
  "version": "8.0.0",
  "description": "PostCSS plugin to convert css variables that are not redeclared and remove duplicates",
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "variables",
    "optimization",
    "bulma"
  ],
  "files": ["index.js"],
  "scripts": {
    "test": "jest && npm run lint",
    "lint": "eslint . --fix"
  },
  "author": "Adrien Foulon <adrien.foulon@tukan.hu>",
  "license": "MIT",
  "repository": "bulbacss/postcss-var-optimize",
  "dependencies": {
    "postcss-calc": "^8.2.4"
  },
  "peerDependencies": {
    "postcss": "^8.0.0"
  },
  "devDependencies": {
    "eslint": "^8.29.0",
    "eslint-config-postcss": "^4.0.0",
    "eslint-plugin-jest": "^27.1.6",
    "jest": "^29.3.1"
  },
  "eslintConfig": {
    "extends": "eslint-config-postcss",
    "plugins": ["jest"],
    "env": {
      "jest/globals": true
    }
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "jest": {
    "testEnvironment": "node"
